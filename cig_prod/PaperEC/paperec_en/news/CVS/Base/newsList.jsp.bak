<%
    int newsCount=0;
    int totalPage=0;
    String[][] newslist;
    int pageno= (Integer.valueOf(request.getParameter("pageNo"))).intValue();
    int category_id = (Integer.valueOf(request.getParameter("categoryId"))).intValue();  
    
    
    newslist = NewsList.getNewsList();
